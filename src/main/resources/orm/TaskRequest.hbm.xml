<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bo.net.tigo.model.TaskRequest" table="ILN_TASKREQUEST" schema="IMPORTLUCKY">
        <id name="id" type="java.lang.Long">
            <column name="TR_ID"/>
            <generator class="native">
                <param name="sequence">ILN_TASKREQUEST_ID_SEQ</param>
            </generator>
        </id>
        <property name="datetime" type="java.util.Date">
            <column name="ILN_TR_DATETIME"/>
        </property>
        <property name="now" type="java.lang.Boolean">
            <column name="ILN_TR_NOW"/>
        </property>
        <set name="orderRequests" table="ILN_ORDERREQUEST" inverse="true" lazy="true" fetch="select">
            <key>
                <column name="TR_ID" not-null="true"/>
            </key>
            <one-to-many class="bo.net.tigo.model.OrderRequest"/>
        </set>
    </class>
</hibernate-mapping>