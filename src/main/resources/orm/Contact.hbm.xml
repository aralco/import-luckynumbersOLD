<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="bo.net.tigo.model.Contact" table="ILN_CONTACT" schema="IMPORTLUCKY">
        <id name="id" type="java.lang.Long">
            <column name="ILN_CONTACT_ID"/>
            <generator class="native">
                <param name="sequence">ILN_CONTACT_SEQ_ID</param>
            </generator>
        </id>
        <property name="name" type="java.lang.String">
            <column name="ILN_CONTACT_NAME"/>
        </property>
        <property name="email1" type="java.lang.String">
            <column name="ILN_CONTACT_EMAIL1"/>
        </property>
        <property name="email2" type="java.lang.String">
            <column name="ILN_CONTACT_EMAIL2"/>
        </property>
        <property name="phone1" type="java.lang.String">
            <column name="ILN_CONTACT_PHONE1"/>
        </property>
        <property name="phone2" type="java.lang.String">
            <column name="ILN_CONTACT_PHONE2"/>
        </property>
        <property name="phone3" type="java.lang.String">
            <column name="ILN_CONTACT_PHONE3"/>
        </property>
        <property name="description" type="java.lang.String">
            <column name="ILN_CONTACT_DESCRIPTION"/>
        </property>
        <property name="createdDate" type="java.util.Date">
            <column name="ILN_CONTACT_CREATED_DATE"/>
        </property>
        <property name="lastUpdate" type="java.util.Date">
            <column name="ILN_CONTACT_LAST_UPDATE"/>
        </property>
        <property name="enabled" type="java.lang.Boolean">
            <column name="ILN_CONTACT_ENABLED" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>